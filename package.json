{
  "name": "msdf",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:cpp": "emcc --bind -sEXPORTED_FUNCTIONS=\"_malloc\",\"_free\" -sMODULARIZE --extern-post-js post.js -o ../msdf/lib/msdfgen.js wasm.cpp core/Shape.cpp core/EdgeHolder.cpp core/Projection.cpp core/DistanceMapping.cpp core/msdfgen.cpp core/contour-combiners.cpp core/edge-selectors.cpp core/MSDFErrorCorrection.cpp core/Contour.cpp core/msdf-error-correction.cpp core/equation-solver.cpp core/edge-coloring.cpp core/edge-segments.cpp",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "maxrects-packer": "^2.7.3",
    "opentype.js": "^1.3.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.167.0"
  },
  "devDependencies": {
    "@types/opentype.js": "^1.3.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.167.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "prettier": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vite-plugin-glsl": "^1.3.0",
    "vite-plugin-wasm": "^3.3.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
